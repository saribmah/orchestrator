{
  "name": "orchestrator",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "orchestrator": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "lint": "oxlint src/",
    "typecheck": "tsc --noEmit",
    "format": "oxfmt --write src/"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
